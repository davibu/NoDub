{
	"manifest_version": 2,
	"name": "CrunchyrollDubDeleter",
	"version": "1.0",
  
	"description": "Deletes dub listings from the simulcastcalander",
  
	"icons": {
	  "48": "NoDub.png"
	},
  
	"content_scripts": [
	  {
		"matches": ["*://*.crunchyroll.com/simulcastcalendar*"],
		"js": ["removeDub.js"]
	  }
	]
  }
  